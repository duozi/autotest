<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="test-user-service" />

    <dubbo:registry protocol="zookeeper" address="10.10.16.150:2181,10.10.16.150:2182,10.10.16.150:2183"/>

    <dubbo:reference id="iLoginService" interface="cn.xn.user.service.ILoginService" group="p2p" version="1.0"
                     check="false" timeout="30000"/>
    <dubbo:reference id="iFriendService" interface="cn.xn.user.service.IFriendService" group="p2p" version="1.0"
                     check="false" timeout="30000"/>
    <dubbo:reference id="iCustomerInfoService" interface="cn.xn.user.service.ICustomerInfoService" group="p2p" version="1.0"
                     check="false" timeout="30000"/>
    <dubbo:reference id="iPwdService" interface="cn.xn.user.service.IPwdService" group="p2p" version="1.0"
                     check="false" timeout="30000"/>
    <dubbo:reference id="iRegisterService" interface="cn.xn.user.service.IRegisterService" group="p2p" version="1.0"
                     check="false" timeout="30000"/>
    <dubbo:monitor protocol="registry"/>

</beans>
